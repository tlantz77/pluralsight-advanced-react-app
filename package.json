{
  "name": "advanced-react",
  "version": "1.0.0",
  "description": "Advanced React",
  "main": "lib/server.js",
  "repository": "https://github.com/jscomplete/advanced-react",
  "author": "Tim Lantz",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node",
    "webpack": "webpack -wd",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "lodash": "^4.17.11",
    "pm2": "^2.5.0",
    "prop-types": "^15.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^4.0.0"
  }
}
